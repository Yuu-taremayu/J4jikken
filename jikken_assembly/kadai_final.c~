#include "h8-3052-iodef.h"

void sub_1(void)
{
  int i;

  i = 0;
}

int sub_2(void)
{
  return 0;
}

int sub_3(void)
{
  int i;

  i = 1;

  return i;
}

int sub_4(int i)
{
  int j, k;

  j = i;

  if (j == 0) {
    k = 1;
  }
  else if (j == 1) {
    k = 10;
  }
  else {
    k = 100;
  }

  return k;
}

int sub_5(int i)
{
  int j, k, l;

  j = i;
  l = 0;

  for (k = 0; k < j; k++) {
    l = l + k;
  }

  return l;
}

int main(void)
{
  int i;

  sub_1();
  sub_2();
  sub_3();

  sub_4(i);

  sub_5(i);

  return 0;
}
